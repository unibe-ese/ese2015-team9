<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="memberDaoMock" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="team9.tutoragency.model.dao.MemberDao"/>
    </bean>
    <bean id="courseDaoMock" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="team9.tutoragency.model.dao.CourseDao"/>
    </bean>
    <bean id="universityDaoMock" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="team9.tutoragency.model.dao.UniversityDao"/>
    </bean>

    <bean id="searchService" class="team9.tutoragency.controller.service.SearchService">
        <property name="memberDao" ref="memberDaoMock"/>
        <property name="courseDao" ref="courseDaoMock"/>
    </bean>


</beans>